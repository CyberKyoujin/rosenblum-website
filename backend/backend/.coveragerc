[run]
source = base
omit =
    */migrations/*
    */tests/*
    */venv/*
    */virtualenv/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

[paths]
source =
    base/
    /app/base/
